{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "book_id",
        "student_id",
        "date"
    ],
    "properties": {
        "book_id": {
            "type": "string",
            "title": "The book_id Schema",
            "examples": [
                101
            ]
        },
        "student_id": {
            "type": "string",
            "title": "The student_id Schema",
            "examples": [
                1001
            ]
        },
        "date": {
            "type": "string",
            "title": "The date Schema",
            "examples": [
                "10-24-2023"
            ]
        }
    },
    "additionalProperties": false,
    "examples": [{
        "book_id": 101,
        "student_id": 1001,
        "date": "10-24-2023"
    }]
}